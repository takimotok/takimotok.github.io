---
import { Image } from 'astro:assets'
import PrflImg from '@/assets/prfl.jpg'

interface Props {
  alt: string
  width?: number
  height?: number
}

const { alt, width = 192, height = 192 } = Astro.props
---

<Image src={PrflImg} alt={alt} width={width} height={height} class="h-48 w-48 rounded-full drop-shadow-md" />
